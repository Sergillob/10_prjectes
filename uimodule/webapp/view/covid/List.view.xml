<mvc:View controllerName="com.te.teProjects.controller.covid.List" displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:l="sap.ui.layout"
  xmlns:grid="sap.ui.layout.cssgrid"
  xmlns:core="sap.ui.core" height="100%">
  <Page id="covid" showHeader="false">

    <List id="myList" headerText="Total Cases {Latest>/data/summary/total}" footerText="Latest Covid Count" items="{Latest>/data/regional}">
      <items>
        <StandardListItem title="{Latest>loc}" description="Total Cases {Latest>totalConfirmed}" info="Deaths {Latest>deaths}" />
      </items>
    </List>



  </Page>
</mvc:View>